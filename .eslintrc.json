{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "max-len": "off",
        "no-console": "warn",
        "comma-dangle": "off",
        "prettier/prettier": "warn",
        "no-unused-vars": "warn"
    }
}
